    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "expo": "^42.0.0",
    "expo-calendar": "~9.2.2",
    "expo-cli": "^3.28.5",
    "expo-constants": "~11.0.1",
    "expo-haptics": "~10.1.0",
    "expo-image-picker": "~10.2.2",
    "expo-localization": "~10.2.0",
    "expo-location": "~12.1.2",
    "expo-notifications": "~0.12.3",
    "expo-permissions": "~12.1.1",
    "firebase": "8.2.3",
