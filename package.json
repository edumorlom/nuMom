    "react-native-dropdown-picker": "^3.7.1",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-haptic-feedback": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
