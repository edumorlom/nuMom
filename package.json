    "@react-navigation/stack": "^5.9.3",
    "expo": "^45.0.8",
    "expo-calendar": "~10.2.0",
    "expo-constants": "~13.1.1",
    "expo-haptics": "~11.2.0",
    "expo-image-picker": "~13.1.1",
