    "expo-haptics": "^8.3.0",
    "expo-image-picker": "~9.1.0",
    "expo-localization": "~9.0.0",
    "expo-location": "~9.0.0",
    "expo-permissions": "~9.3.0",
    "firebase": "^7.21.0",
