    "ios": "./node_modules/.bin/expo start --ios",
    "web": "./node_modules/.bin/expo start --web",
    "eject": "./node_modules/.bin/expo eject",
    "fix": "npm run format && npm run esfix",
    "esfix": "./node_modules/.bin/eslint --fix \"**/*.+(js|jsx)\"",
